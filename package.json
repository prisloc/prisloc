{
  "name": "prisloc",
  "version": "0.1.0",
  "description": "A local-first Prisma-like ORM for TypeScript",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.0.0",
    "turbo": "^2.3.2",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=20.0.0"
  },
  "bugs": {
    "url": "https://github.com/sirlucasm/prisloc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirlucasm/prisloc.git"
  },
  "homepage": "https://github.com/sirlucasm/prisloc#readme",
  "author": "sirlucasm",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "orm",
    "typescript",
    "database",
    "local",
    "prisma",
    "typeorm"
  ]
}
