{
  "name": "prisloc",
  "version": "0.1.22",
  "description": "A local-first Prisma-like ORM for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "rimraf .turbo node_modules dist coverage",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "prisloc": "bin/prisloc"
  },
  "keywords": [
    "orm",
    "typescript",
    "database",
    "local",
    "prisma",
    "typeorm"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/uuid": "^9.0.2",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^7.1.0"
  },
  "bugs": {
    "url": "https://github.com/sirlucasm/prisloc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirlucasm/prisloc.git"
  },
  "homepage": "https://github.com/sirlucasm/prisloc#readme",
  "author": "sirlucasm",
  "publishConfig": {
    "access": "public"
  }
}
