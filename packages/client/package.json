{
  "name": "prisloc",
  "version": "0.1.4",
  "description": "A local-first Prisma-like ORM for TypeScript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/**/*",
    "dev": "tsup src/**/* --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "clean": "rimraf .turbo node_modules dist coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "orm",
    "typescript",
    "database",
    "local",
    "prisma",
    "typeorm"
  ],
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/uuid": "^9.0.2",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "tsup": "^7.1.0"
  },
  "files": [
    "dist/index.js",
    "dist/types.js"
  ],
  "bugs": {
    "url": "https://github.com/sirlucasm/prisloc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirlucasm/prisloc.git"
  },
  "homepage": "https://github.com/sirlucasm/prisloc#readme",
  "author": "sirlucasm",
  "publishConfig": {
    "access": "public"
  }
}
